{
  "info": {
    "name": "PRODO RAG - Render",
    "_postman_id": "prodo-rag2-render-collection",
    "description": "Postman collection for testing PRODO RAG endpoints on Render",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Home",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/",
          "host": ["{{base_url}}"],
          "path": [""]
        }
      }
    },
    {
      "name": "Health",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/rag/health",
          "host": ["{{base_url}}"],
          "path": ["rag", "health"]
        }
      }
    },
    {
      "name": "Index Repo",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"repoId\": \"test-repo\",\n  \"files\": [\n    {\"filename\": \"test.py\", \"content\": \"print('Hello World')\"}\n  ],\n  \"metadata\": {}\n}"
        },
        "url": {
          "raw": "{{base_url}}/rag/index",
          "host": ["{{base_url}}"],
          "path": ["rag", "index"]
        }
      }
    },
    {
      "name": "Delete Repo Vectors",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": {
          "raw": "{{base_url}}/rag/delete?repoId={{repoId}}",
          "host": ["{{base_url}}"],
          "path": ["rag", "delete"],
          "query": [
            {
              "key": "repoId",
              "value": "{{repoId}}"
            }
          ]
        }
      }
    },
    {
      "name": "Reset Repo",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": {
          "raw": "{{base_url}}/rag/reset?repoId={{repoId}}",
          "host": ["{{base_url}}"],
          "path": ["rag", "reset"],
          "query": [
            {
              "key": "repoId",
              "value": "{{repoId}}"
            }
          ]
        }
      }
    },
    {
      "name": "RAG Query",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"repoId\": \"test-repo\",\n  \"prompt\": \"Explain the following code and give improvements:\\nprint(\\\"Hello\\\")\",\n  \"top_k\": 6,\n  \"metadata\": {}\n}"
        },
        "url": {
          "raw": "{{base_url}}/rag/query",
          "host": ["{{base_url}}"],
          "path": ["rag", "query"]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "https://prodo-rag2.onrender.com",
      "type": "string"
    },
    {
      "key": "repoId",
      "value": "test-repo",
      "type": "string"
    }
  ]
}
